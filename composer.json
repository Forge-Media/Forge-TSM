{
    "name": "forge-media/forge-tsm",
    "type": "web-app",
    "description": "Simple web-front which allows the creation of channels on a Teamspeak 3 server",
    "keywords": ["ts3", "php", "heroku", "c9.io", "teamspeak3"],
    "minimum-stability": "dev",
    "homepage": "https://github.com/forge-media/forgetsm-php-framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Forge Media"
        },
        {
            "name": "Forge Media",
            "email": "admin@forgegaming.net"
        }
    ],
    "engines": {
        "npm": "4.1.1"
    },
    "dependencies": {
        "bower": "1.7.2"
    },
    "require": {
        "php": ">=5.5",
        "forge-media/forgetsm-php-framework": "dev-master"
    },
    "scripts": {
        "start": "node index.js",
        "test": "mocha",
        "postinstall": "bower install && grunt build",
        "post-install-cmd": [
            "bower install"
        ],
        "post-update-cmd": [
            "bower install"
        ]
    }
}
